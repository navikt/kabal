{
  "name": "nav-frontend-example-webpack",
  "version": "0.0.1",
  "description": "Webpack setup for frontendprosjekter",
  "main": "index.tsx",
  "scripts": {
    "clean": "rimraf dist",
    "build:js": "webpack --config ./webpack.config.js --mode production",
    "build:html": "copyfiles -f ./public/index.html dist",
    "build": "npm run clean && npm run build:js && npm run build:html",
    "start": "better-npm-run start",
    "test": "echo \"No test specified\" && exit 0",
    "lint": "npx eslint src --ext .js,.jsx --color"
  },
  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --config ./webpack.config.js --mode development",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "author": "SA <sven.anders.robbestad@nav.no>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@types/axios": "^0.14.0",
    "@types/qs": "^6.9.5",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.0",
    "css-loader": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.1.0",
    "husky": "^4.3.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "better-npm-run": "^0.1.1",
    "classnames": "^2.2.6",
    "nav-frontend-alertstriper": "^3.0.23",
    "nav-frontend-alertstriper-style": "^2.0.17",
    "nav-frontend-core": "^5.0.1",
    "nav-frontend-ikoner-assets": "^2.0.3",
    "nav-frontend-lenker": "^1.0.41",
    "nav-frontend-lenker-style": "^0.2.29",
    "nav-frontend-paneler": "^2.0.10",
    "nav-frontend-paneler-style": "^0.3.21",
    "nav-frontend-typografi": "^2.0.26",
    "nav-frontend-typografi-style": "^1.0.22",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
